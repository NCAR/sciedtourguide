  "postcard": {
    "intro": {
      "content": "<h1>Welcome to the postcard generator.</h1><p>Please follow the instructions provided to create your postcard.</p>"
    },
    "steps":{
      "one":{
        "instructions":{
          "content":"First, select a background image."
        }
      },
      "two" :{
        "instructions":{
          "content": "This is your selected background image. You can choose another background image or move to the next step and take a photo of yourself."
        },
        "buttons":{
          "take_picture": "Take a photo",
          "camera_back": "Choose a different image"
        }
      },
      "three" :{
        "instructions":{
            "content": "This is your photo.  Nice shot!  You can retake your photo or write your message.  Please limit to 140 characters or less."
        },
        "message":{
            "label": "Message",
            "placeholder_text":"Example: Hi Mom!  I just visited the NCAR Visitor Center and played with a cloud!"
        },
        "buttons":{
          "finish": "Finish my postcard",
          "message_back": "Take a different photo"
        }
      },
      "finished":{
        "loading_text":"Creating your postcard...",
        "review_text":"Here is your postcard.  Would you like send it?",
        "buttons":{
          "send":"Send my postcard",
          "startover":"Start Over"
        }
      }
    }
