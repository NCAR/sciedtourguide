  "visitor": {
    "title": "Visitor Information",
    "intro": {
      "content": "The NCAR Visitor Center is located at the NCAR Mesa Lab in Boulder, Colorado. Admission is always free.",
      "button_text": "Visitor website"
    },
    "address": {
      "title": "Address",
      "content": "1850 Table Mesa Drive<br/>Boulder, Colorado 80305",
      "button_text": "View directions"
    },
    "hours": {
      "title": "Visitor Center Hours",
      "content": "Weekdays: 8:00 am - 5:00 pm  <br/> Weekends: 9:00 am - 4:00 pm  <br/> Holidays: 9:00 am - 4:00 pm  <p>The visitor center is open every day except for two annual shut down days in the spring and fall. View our visitor website for updates when these shut downs are scheduled.</p>"
    },
    "tours": {
      "title": "Tours",
      "content": "Public tours, MWF: 12:00 pm",
      "button_text": "View tour information"
    },
    "cafeteria": {
      "title": "Cafeteria Hours",
      "content": "Breakfast, 8:00 am - 9:30 am<br/>Lunch, 11:30 - 1:30 pm  <p>The cafeteria is open weekdays only, except holidays. The cafeteria accepts only cash.</p>"
    },
    "accessibility": {
      "title": "Accessibility",
      "content": "The NCAR Mesa Lab Visitor Center is committed to opening its doors to everyone, including those with disabilities along with their families and friends.  Our Visitor program is committed to ensuring an inclusive environment which promotes learning and enjoyment for visitors with diverse interests, abilities, and needs.  Additional information about our accessibility accomodations can be found on our visitor website.",
      "button_text": "View accessibility information",
      "parking": {
        "title": "Parking",
        "content": "NCAR's Mesa Lab Visitor Center offers two entrances for those with mobility impairments and their companions.",
        "title1": "Front driveway access",
        "content1": "This is a secure driveway and entrance, therefore one must call NCAR (303.497.1139) in advance to gain access to the two accessible parking spaces or to drop visitors off at the main entrance.",
        "title2": "Accessible rear entrance",
        "content2": "The second option is to park in the accessible parking area at the back (south side) of the building directly next to our delivery dock. Please buzz the intercom located next to the inside door and our security staff will facilitate your entrance. (Please feel free to ask via the intercom for a staff escort to meet you at this entrance if desired.) All doors in this area are automatic. Once inside, follow the green dots to the elevator. Enter the elevator, press \"G\" for Ground Level, and it will transport you to the first floor of our Visitor Center."
      },
      "elevators": {
        "title": "Elevators ",
        "content": "All exhibits and public areas are accessible by elevator."
      },
      "restrooms": {
        "title": "Restrooms ",
        "content": "A wheelchair accessible restroom is available on the second floor in the Climate Future exhibit area."
      },
      "map": {
        "button_text": "View accessibility map"
      }
    }
  },
